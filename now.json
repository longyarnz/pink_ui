{
  "version": 2,
  "name": "pink_ui",
  "alias": [ "test.pinkettu.com.ng" ],
  "builds": [
    { "src": "*.html", "use": "@now/html-minifier" },
    { "src": "**/*.js", "use": "@now/node" },
    { "src": "*/*.css", "use": "@now/html-minifier" },
    { "src": "*/*.png", "use": "@now/optipng" },
    { "src": "images/*.jpg", "use": "@now/static" },
    { "src": "fonts/*.woff", "use": "@now/static" },
    { "src": "fonts/*.woff2", "use": "@now/static" }
  ]
}
